{
  "name": "react-mosaic-component",
  "version": "0.0.1",
  "description": "A React Tiling Window Manager",
  "license": "Apache-2.0",
  "keywords": [
    "ui",
    "react",
    "component",
    "typescript",
    "tiling-window-manager",
    "window-manager"
  ],
  "scripts": {
    "nx": "nx",
    "start": "npx nx run demo-app:serve",
    "lint": "nx run-many -t lint",
    "test": "nx run-many -t test",
    "format": "nx format:write",
    "update": "cross-env NX_SKIP_PROVENANCE_CHECK=true npx nx migrate latest && npm install && npx nx migrate --run-migrations",
    "build:app": "npx nx run demo-app:build --base ./",
    "build:lib": "npm-run-all clean -lp build-lib:** && run-s copy",
    "build-lib:less": "lessc --autoprefix=defaults ./libs/react-mosaic-component/src/lib/styles/index.less ./dist/libs/react-mosaic-component/react-mosaic-component.css",
    "build-lib:js": "tsup",
    "build-lib:check": "tsc -p ./libs/react-mosaic-component/tsconfig.lib.json --noEmit",
    "build-lib:types": "tsup",
    "clean": "shx rm -rf ./dist/libs/react-mosaic-component/",
    "copy": "shx cp ./libs/react-mosaic-component/package.json ./dist/libs/react-mosaic-component/package.json && shx cp README.md ./dist/libs/react-mosaic-component/README.md && shx cp LICENSE ./dist/libs/react-mosaic-component/LICENSE",
    "prepare": "lefthook install",
    "release": "npm run lint && npm run test && nx release --verbose --skip-publish"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "dnd-core": "16.0.1",
    "immutability-helper": "^3.1.1",
    "lodash-es": "^4.17.21",
    "prop-types": "^15.8.1",
    "rdndmb-html5-to-touch": "^9.0.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dnd-multi-backend": "^9.0.0",
    "react-dnd-touch-backend": "^16.0.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.2",
    "@babel/core": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@blueprintjs/core": "^5.7.0",
    "@blueprintjs/icons": "^5.19.1",
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "@eslint/js": "^9.8.0",
    "@nx/eslint": "22.3.3",
    "@nx/eslint-plugin": "22.3.3",
    "@nx/js": "22.3.3",
    "@nx/react": "22.3.3",
    "@nx/vite": "22.3.3",
    "@nx/vitest": "22.3.3",
    "@nx/web": "22.3.3",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@testing-library/dom": "10.4.1",
    "@testing-library/react": "16.3.0",
    "@types/classnames": "^2.3.1",
    "@types/dom4": "^2.0.2",
    "@types/lodash-es": "4.17.12",
    "@types/node": "^20.0.0",
    "@types/prop-types": "^15.7.5",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.8",
    "@types/uuid": "^9.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "4.0.0",
    "@vitest/ui": "4.0.0",
    "ajv": "^8.0.0",
    "cross-env": "7.0.3",
    "css-loader": "^6.7.3",
    "esbuild-plugin-file-path-extensions": "^2.1.2",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "jsdom-global": "^3.0.2",
    "lefthook": "^1.10.0",
    "less": "^3.13.1",
    "less-plugin-autoprefix": "^2.0.0",
    "mock-require": "^3.0.3",
    "npm-run-all2": "8.0.4",
    "nx": "22.3.3",
    "nx-github-pages": "^1.0.0",
    "prettier": "3.5.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-refresh": "^0.17.0",
    "shx": "0.3.4",
    "tsup": "^8.5.0",
    "typescript": "5.5.4",
    "typescript-eslint": "8.40.0",
    "verdaccio": "^6.0.5",
    "vite": "7.3.1",
    "vite-plugin-dts": "~4.5.0",
    "vite-plugin-static-copy": "^3.0.2",
    "vitest": "4.0.0"
  },
  "peerDependencies": {
    "react": "16 - 19"
  }
}
